<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="niad.kr.example50">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.TransitionActivity" />
        <activity android:name=".activity.TransitionDetailActivity" />
        <activity android:name=".activity.SharedElementsActivity" />
        <activity android:name=".activity.SharedElementsDetailActivity" />
        <activity android:name=".activity.RecyclerViewWithEndlessLoaderActivity" />
        <activity android:name=".activity.AnimationActivity" />
        <activity android:name=".activity.SceneBasedAnimationActivity" />
        <activity android:name=".activity.RevealActivity"></activity>
    </application>

</manifest>